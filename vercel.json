{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/menu/(.*)",
      "dest": "/api/menu?id=$1"
    },
    {
      "src": "/(.*\\.(png|jpg|jpeg|gif|svg|css|js))",
      "dest": "/public/$1"
    }
  ]
}
